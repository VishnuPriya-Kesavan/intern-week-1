#include<iostream>
#include <bits/stdc++.h>
using namespace std;
int main(){
    string S;
    cout<<"Enter string in lower case:";
    cin>>S;
    string r=S;
	   reverse(r.begin(),r.end());
	    if(r==S){
	        cout<<"Is a palindrome";
	    }
	    else{
	        cout<<"Is  not a palindrome";  
	    }
}
